  "author": "Creatide",
  "authorEmail" : "hello@creatide.com",
  "homepage": "http://github.com/Creatide/Sketch_Update-Text-Layer-Name",
  "version": 1.0,
  "identifier": "com.creatide.sketch.sync-text-layer-name",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Sync Text Layer Name",
